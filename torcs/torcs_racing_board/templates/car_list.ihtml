
<table class="embedded" cellspacing="0">
	<tr>
		<td class="border contentbordercolor">
			<h3>Car List</h3>
			<p>
				List of the cars available for races on the TORCS racing board.
			</p>
		</td>
	</tr>
</table>

<table class="embedded noborder text14" cellspacing="0">
	<tr class="submitbuttoncolor">
		<th class="filledlist" align="left">Car</th>
		<th class="filledlist" align="left">Drive Train</th>
		<th class="filledlist" align="left">Mass [kg]</th>
		<th class="filledlist" align="left">Power [kW]</th>
	</tr>
	<!-- BEGIN row -->
	<tr bgcolor="{PC_COLOR}">
		<td class="filledlist">
			<a href="{ROOTPATH}cars/car_view.php?viewcarid={PC_CAR_ID}">{PC_CAR_NAME}</a>
		</td>
		<td class="filledlist">
			{PC_CAR_TYPE}
		</td>
		<td class="filledlist">
			{PC_CAR_MASS}
		</td>
		<td class="filledlist">
			{PC_CAR_POWER}
		</td>
	</tr>
	<!-- END row -->
</table>

<table class="embedded noborder navmargin text14" cellspacing="0">
	<tr>
		<td class="filledlist" style="text-align:left;">
			<a href="{PC_LIST_PREV_LINK}">{PC_LIST_PREV}</a>
		</td>
		<td class="filledlist" style="text-align:right;">
			<a href="{PC_LIST_NEXT_LINK}">{PC_LIST_NEXT}</a>
		</td>
	</tr>
</table>
